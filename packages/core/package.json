{
  "name": "vite-plugin-eslint2",
  "version": "4.4.0",
  "description": "ESLint plugin for Vite.",
  "keywords": [
    "eslint",
    "vite-plugin"
  ],
  "homepage": "https://github.com/ModyQyW/vite-plugin-eslint2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com:ModyQyW/vite-plugin-eslint2.git",
    "directory": "packages/core"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui.dev@gmail.com",
    "url": "https://modyqyw.top"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "prepublishOnly": "pnpm build",
    "stub": "unbuild --stub"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "chokidar": "^3.6.0",
    "debug": "^4.3.7"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/eslint": "^8.56.12",
    "@types/node": "^20.16.5",
    "picocolors": "^1.1.0",
    "rollup": "^4.21.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.4"
  },
  "peerDependencies": {
    "@types/eslint": "^7.0.0 || ^8.0.0 || ^9.0.0-0",
    "eslint": "^7.0.0 || ^8.0.0 || ^9.0.0-0",
    "rollup": "^2.0.0 || ^3.0.0 || ^4.0.0",
    "vite": "^2.0.0 || ^3.0.0 || ^4.0.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "@types/eslint": {
      "optional": true
    },
    "rollup": {
      "optional": true
    }
  },
  "packageManager": "pnpm@9.10.0",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
