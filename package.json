{
  "name": "vite-plugin-eslint2",
  "version": "3.2.1",
  "description": "ESLint plugin for Vite.",
  "keywords": [
    "eslint",
    "vite-plugin"
  ],
  "homepage": "https://github.com/ModyQyW/vite-plugin-eslint2",
  "repository": "git+https://github.com:ModyQyW/vite-plugin-eslint2.git",
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://modyqyw.github.io"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "check:deps": "ncu --dep dev,optional,prod,packageManager",
    "check:types": "tsc --noEmit",
    "commit": "git-cz",
    "dev": "unbuild --stub",
    "examples:react:build": "pnpm --filter react run build",
    "examples:react:dev": "pnpm --filter react run dev",
    "examples:solid:build": "pnpm --filter solid run build",
    "examples:solid:dev": "pnpm --filter solid run dev",
    "examples:vue:build": "pnpm --filter vue run build",
    "examples:vue:dev": "pnpm --filter vue run dev",
    "lint": "run-p lint:eslint lint:markdownlint check:types",
    "lint:eslint": "eslint ./src --fix --cache",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "pnpm run build",
    "release": "pnpm install && pnpm run lint && bumpp"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.0.2",
    "chokidar": "^3.5.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.3",
    "@commitlint/config-conventional": "^17.4.3",
    "@commitlint/prompt": "^17.4.3",
    "@modyqyw/fabric": "^7.0.3",
    "@modyqyw/utils": "^4.1.0",
    "@tsconfig/node18": "^1.0.1",
    "@types/eslint": "^8.21.1",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "bumpp": "^9.0.0",
    "eslint": "^8.34.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.1.2",
    "markdownlint-cli": "^0.33.0",
    "npm-run-all": "^4.1.5",
    "picocolors": "^1.0.0",
    "prettier": "^2.8.4",
    "rollup": "^3.15.0",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.2",
    "vite": "^4.1.1"
  },
  "peerDependencies": {
    "@types/eslint": "^7.0.0 || ^8.0.0",
    "eslint": "^7.0.0 || ^8.0.0",
    "rollup": "^2.0.0 || ^3.0.0",
    "vite": "^2.0.0 || ^3.0.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "rollup": {
      "optional": true
    }
  },
  "packageManager": "pnpm@7.27.0",
  "engines": {
    "node": ">=14.18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
